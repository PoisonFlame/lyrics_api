n          }\n        }\n      },\n    },\n  },\n  methods: {\n    onCvChange(srcComponent) {\n      const index = this.$children.findIndex(item => item.uid === srcComponent.uid);\n      this.$emit('change', { changedIndex: index, state: this.state });\n    },\n  },\n};\n</script>\n"]}]}